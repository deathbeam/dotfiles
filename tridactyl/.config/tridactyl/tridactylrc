" For syntax highlighting see https://github.com/tridactyl/vim-tridactyl
" vim: set filetype=tridactyl

" Disable GUI
" guiset gui none

" General Settings
set configversion 2.0
set theme solarized
set allowautofocus false
set incsearch true
set modeindicatorshowkeys true

" Swap J and K because of vertical tabs
bind J tabnext
bind K tabprev

" Stuff
set editorcmd $TERMINAL -e nvim
